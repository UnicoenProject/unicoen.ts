{
  "name": "unicoen.ts",
  "version": "0.1.11",
  "description": "unicoen.ts",
  "main": "dist/index.js",
  "typings": "dist/index.d.ts",
  "private": false,
  "repository": {
    "type": "git",
    "url": "git+https://github.com/UnicoenProject/unicoen.ts.git"
  },
  "bugs": {
    "url": "https://github.com/UnicoenProject/unicoen.ts/issues"
  },
  "homepage": "https://github.com/UnicoenProject/unicoen.ts#readme",
  "author": "Ryosuke Ishizue",
  "license": "MIT",
  "scripts": {
    "build": "rimraf dist && tsc",
    "watch": "rimraf dist && tsc -w",
    "format": "prettier --write src/**.ts **/*.json",
    "prepare": "yarn build",
    "generate:node": "ts-node src/node/Generator.ts",
    "precommit": "lint-staged",
    "postcommit": "git update-index --again",
    "test": "yarn build && yarn exec:test",
    "exec:test": "mocha dist/*.spec.js"
  },
  "devDependencies": {
    "@types/chai": "^4.1.5",
    "@types/mocha": "^2.2.48",
    "chai": "^4.2.0",
    "lint-staged": "^7.3.0",
    "mocha": "^4.1.0",
    "post-commit": "^0.0.1",
    "pre-commit": "^1.2.2",
    "prettier": "^1.14.3",
    "rimraf": "^2.6.2",
    "ts-node": "^7.0.1",
    "tslint": "^5.9.1",
    "typescript": "^3.1.1"
  },
  "dependencies": {
    "@types/node": "^9.6.32",
    "agh.sprintf": "0.0.11",
    "antlr4ts": "^0.4.1-alpha.0",
    "scanf": "^1.0.2"
  },
  "pre-commit": "precommit",
  "post-commit": "postcommit",
  "lint-staged": {
    "*.{js,json}": [
      "prettier --write",
      "git add"
    ],
    "*.ts": [
      "prettier --write",
      "tslint --fix",
      "git add"
    ]
  }
}
