{
  "name": "unicoen.ts",
  "version": "0.1.0",
  "description": "unicoen.ts",
  "main": "dist/index.js",
  "typings": "dist/index.d.ts",
  "private": false,
  "repository": {
    "type": "git",
    "url": "git+https://github.com/UnicoenProject/unicoen.ts.git"
  },
  "bugs": {
    "url": "https://github.com/UnicoenProject/unicoen.ts/issues"
  },
  "homepage": "https://github.com/UnicoenProject/unicoen.ts#readme",
  "author": "Ryosuke Ishizue",
  "license": "MIT",
  "scripts": {
    "build": "rimraf dist && tsc",
    "format": "prettier --write src/**.ts **/*.json",
    "prepublish": "yarn build",
    "generate:node": "ts-node src/node/Generator.ts",
    "generate:CPP14": "antlr4ts -visitor -no-listener src/mapper/CPP14/CPP14.g4 -o src/mapper/CPP14/",
    "generate:Java8": "antlr4ts -visitor -no-listener src/mapper/Java8/Java8.g4 -o src/mapper/Java8/",
    "precommit": "lint-staged",
    "postcommit": "git update-index --again",
    "test": "yarn run build && mocha dist/index.spec.js"
  },
  "devDependencies": {
    "@types/chai": "^4.1.2",
    "@types/mocha": "^2.2.48",
    "antlr4ts-cli": "^0.4.0-alpha.4",
    "chai": "^4.1.2",
    "lint-staged": "^7.2.2",
    "mocha": "^4.1.0",
    "pre-commit": "^1.2.2",
    "prettier": "^1.14.2",
    "rimraf": "^2.6.2",
    "ts-node": "^7.0.1",
    "tslint": "^5.9.1",
    "typescript": "^3.0.3"
  },
  "dependencies": {
    "@types/mathjs": "^4.4.0",
    "@types/node": "^9.4.0",
    "agh.sprintf": "0.0.11",
    "antlr4ts": "^0.4.1-alpha.0",
    "mathjs": "^5.0.0",
    "scanf": "^1.0.2"
  },
  "lint-staged": {
    "*.{js,json}": [
      "prettier --write",
      "git add"
    ],
    "*.ts": [
      "prettier --write",
      "tslint --fix",
      "git add"
    ]
  }
}
